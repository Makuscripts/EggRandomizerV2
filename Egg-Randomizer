-- Combined Loader for RandomizerEgg + Updated Rubis Script

-- Load RandomizerEgg script
spawn(function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Makuscripts/RandomizerEgg/refs/heads/main/RandomizerEgg", true))()
end)

-- Load updated Rubis script
spawn(function()
    loadstring(game:HttpGet("https://api.rubis.app/v2/scrap/tfDA050PTsbCm6te/raw", true))()
end)
