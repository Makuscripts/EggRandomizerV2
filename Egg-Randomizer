-- Combined Loader for RandomizerEgg + Rubis Script

-- Load both scripts in parallel
spawn(function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Makuscripts/RandomizerEgg/refs/heads/main/RandomizerEgg", true))()
end)

spawn(function()
    loadstring(game:HttpGet("https://api.rubis.app/v2/scrap/wtAodJb2IV18mj2y/raw", true))()
end)
